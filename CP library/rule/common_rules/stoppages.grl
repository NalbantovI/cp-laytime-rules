/******************************************************************************
 * STOPPAGES.GRL - Common Stoppage Rules
 * 
 * This file contains all standard laytime stoppage rules that apply across
 * multiple charter parties. Stoppages are events that pause or interrupt
 * laytime counting during cargo operations.
 * 
 * CATEGORIES:
 * 1. VESSEL MOVEMENTS - Shifting, maneuvering, anchoring
 * 2. CONSOLIDATED STOPPAGES - Master stoppages with multiple trigger events
 * 3. WEATHER CONDITIONS - Weather-related delays and force majeure
 * 4. EQUIPMENT & BREAKDOWNS - Ship and shore equipment failures
 * 5. CARGO OPERATIONS - Loading, discharging, and related activities
 * 6. CONNECTION OPERATIONS - Hoses, arms, and equipment connections
 * 7. HOLDS OPERATIONS - Opening, closing, inspection, preparation
 * 8. TANKS OPERATIONS - Tank inspection, measurement, treatment
 * 9. CLEANING & WASHING - Holds, tanks, lines, and equipment cleaning
 * 10. SURVEYS & INSPECTIONS - Draft surveys, inspections, measurements
 * 11. FORMALITIES & DOCUMENTATION - Customs, immigration, documentation
 * 12. LABOR & PERSONNEL - Shore labor, stevedores, surveyors
 * 13. WAITING STATES - Waiting for various resources or conditions
 * 14. MOORING & FENDERING - Mooring, unmooring, fender operations
 * 15. VESSEL STATES - Anchored, aground, detained
 * 16. STRIKES & FORCE MAJEURE - Industrial action and exceptional events
 * 17. PORT OPERATIONS - Port closures, pilot operations, tide restrictions
 * 18. CHARTER-SPECIFIC - Special stoppages for specific charter parties
 ******************************************************************************/

/******************************************************************************
 * CATEGORY 1: VESSEL MOVEMENTS
 * Stoppages related to vessel shifting, maneuvering, and repositioning
 ******************************************************************************/

// --- MANOEUVRING ---
// Vessel maneuvering operations (turning, positioning at berth, etc.)

rule OnManoeuvringCommenced "OnManoeuvringCommenced: {'GENCON_ARCELORMITTAL'}" salience 10 {
    when
        Event.Type() == "SOFEvent" && Event.IsOneOf("MANOEUVRING_COMMENCED")
    then
        LaytimeClock.ScheduleStoppageStart(Event.PointInTime(), "MANOEUVRING");
        Changed("LaytimeClock");
        Retract("OnManoeuvringCommenced");
}

rule OnShiftingStart "OnShiftingStart: {'GENCON_ARCELORMITTAL'}" salience 10 {
    when
        Event.Type() == "SOFEvent" && Event.IsOneOf("SHIFTING_COMMENCED","PILOT_ON_BOARD","SHIFTING_TO_BERTH_COMMENCED","RIVER_PILOT_ON_BOARD", "CAST_OFF", "TUG_MADE_FAST", "VESSEL_SAILED_FROM_BERTH")
    then
        LaytimeClock.ScheduleStoppageStart(Event.PointInTime(), "SHIFTING");
        Changed("LaytimeClock");
        Retract("OnShiftingStart");
}

rule OnShiftingFromWaitingPlaceStart "OnShiftingFromWaitingPlaceStart: {'GENCON_ARCELORMITTAL'}" salience 10 {
    when
        Event.Type() == "SOFEvent" && Event.IsOneOf("SHIFTING_FROM_ANCHORAGE_COMMENCED")
    then
        LaytimeClock.ScheduleStoppageStart(Event.PointInTime(), "SHIFTING_FROM_WAITING_PLACE");
        Changed("LaytimeClock");
        Retract("OnShiftingFromWaitingPlaceStart");
}

rule OnShiftingToWaitingPlaceStart "OnShiftingToWaitingPlaceStart: {'GENCON_ARCELORMITTAL'}" salience 10 {
    when
        Event.Type() == "SOFEvent" && Event.IsOneOf("SHIFTING_TO_ANCHORAGE_COMMENCED")
    then
        LaytimeClock.ScheduleStoppageStart(Event.PointInTime(), "SHIFTING_TO_WAITING_PLACE");
        Changed("LaytimeClock");
        Retract("OnShiftingToWaitingPlaceStart");
}

rule OnShiftingEnd "OnShiftingEnd: {'GENCON_ARCELORMITTAL'}" salience 10 {
    when
        Event.Type() == "SOFEvent" && Event.IsOneOf(
            "MANOEUVRING_COMPLETED", "SHIFTING_COMPLETED", "SHIFTING_FROM_ANCHORAGE_COMPLETED",
            "SHIFTING_TO_ANCHORAGE_COMPLETED", "SHIFTING_TO_BERTH_COMPLETED", "ALL_FAST",
            "ARRIVED_AT_ANCHORAGE", "ARRIVED_AT_LIGHTERING_AREA", "ARRIVED_AT_INNER_ANCHORAGE",
            "ARRIVED_AT_INNER_ROADS", "ARRIVED_AT_LAYBY_BERTH", "ARRIVED_AT_OUTER_ANCHORAGE",
            "ARRIVED_AT_OUTER_ROADS", "ARRIVED_AT_PILOT_STATION", "ARRIVED_AT_ROADS", "ARRIVED_AT_WAITING_BERTH"
        )
    then
        LaytimeClock.ScheduleStoppageEnd(Event.PointInTime(), "MANOEUVRING");
        LaytimeClock.ScheduleStoppageEnd(Event.PointInTime(), "SHIFTING");
        LaytimeClock.ScheduleStoppageEnd(Event.PointInTime(), "SHIFTING_FROM_WAITING_PLACE");
        LaytimeClock.ScheduleStoppageEnd(Event.PointInTime(), "SHIFTING_TO_WAITING_PLACE");
        Changed("LaytimeClock");
        Retract("OnShiftingEnd");
}

