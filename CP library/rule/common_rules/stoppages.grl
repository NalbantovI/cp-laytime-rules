// --- CONSOLIDATED: ICE ---							
// Consolidates Ice Conditions and Vessel Stuck in Ice
// Covers any delays due to ice in port or navigation channels
rule OnIceDelayStart "OnIceDelayStart" salience 10 {							
when							
Event.Type() == "SOFEvent" && Event.IsOneOf("ICE_CONDITIONS_STARTED", "VESSEL_STUCK_IN_ICE_START")							
then							
LaytimeClock.ScheduleStoppageStart(Event.PointInTime(), "ICE");							
Changed("LaytimeClock");							
Retract("OnIceDelayStart");							
}							
							
rule OnIceDelayEnd "OnIceDelayEnd" salience 10 {							
